<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.hyh.dao.CourseDao">
    <cache type="com.baizhi.hyh.cache.MybatiesCache"></cache>
    <!--根据用户id查询对应功课-->
    <select id="selectByUid" resultType="Course">
        select *
        from course
        where user_id = #{userId}
    </select>
</mapper>